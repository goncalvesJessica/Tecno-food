{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off",
        "duplicate-id": "off"
      }
    ],
    "button-type": "off",
    "axe/name-role-value": "off",
    "axe/keyboard": "off",
    "axe/forms": "off",
    "disown-opener": "off",
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "width: fit-content",
          "scroll-behavior",
          "@property",
          "@property"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not firefox <= 113",
    "not ios_saf <= 16.5",
    "not safari <= 16.5"
  ]
}